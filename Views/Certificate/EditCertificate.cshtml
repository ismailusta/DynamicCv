@{
    ViewBag.Title = "EditCertificate";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
@using DynamicCv.Models.Entity
@model TblCertificate
<br />
<h4>Edit Certificate Page</h4>
<br />
<form method="post">
    @Html.Label("Certificate Name")
    @Html.TextBoxFor(x => x.Certificate, new { @class = "form-control" })
    <br />
    @Html.Label("Date")
    @Html.TextBoxFor(x => x.Date, new { @class = "form-control" })
    <br />
    <button class="btn-info">Update</button>
</form>