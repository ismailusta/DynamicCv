@{
    ViewBag.Title = "AddAdmin";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h4>Add Admin Page</h4>
<br />
<form method="post">
    <label>User Name</label>
    <input type="text" name="Username" class="form-control" placeholder="Write to here Username..." />
    <br />
    <label>Password</label>
    <input type="text" name="Password" class="form-control" placeholder="Write to here Password..." />
    <br />
    <button class="btn-info">Save</button>
</form>