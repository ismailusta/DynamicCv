@{
    ViewBag.Title = "EditSkill";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
@using DynamicCv.Models.Entity
@model TblSkill
<br />
<h4>Edit Experiment Page</h4>
<br />
<form method="post">
    @Html.Label("Skill Name")
    @Html.TextBoxFor(x => x.Skill, new { @class = "form-control" })
    <br />
    @Html.Label("Rate%")
    @Html.TextBoxFor(x => x.Progress, new { @class = "form-control" })
    <br />
    <button class="btn-info">Update</button>
</form>