@{
    ViewBag.Title = "AddSkill";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h4>Add Skill Page</h4>
<br />
<form method="post">
    <label>Skill Name</label>
    <input type="text" name="Skill" class="form-control" placeholder="Write to here Skill..." />
    <br />
    <label>Rate</label>
    <input type="text" name="Progress" class="form-control" placeholder="Write to here Rate..." />
    <br />
    <button class="btn-info">Save</button>
</form>