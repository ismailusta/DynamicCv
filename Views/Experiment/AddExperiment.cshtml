@{
    ViewBag.Title = "AddExperiment";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h4>Add Experiment Page</h4>
<br />
<form method="post">
    <label>Title</label>
    <input type="text" name="Title" class="form-control" placeholder="Write to here Title..."/>
    <br />
    <label>SubTitle</label>
    <input type="text" name="SubTitle" class="form-control" placeholder="Write to here SubTitle..."/>
    <br />
    <label>GNO</label>
    <input type="text" name="Gno" class="form-control" placeholder="Write to here SubTitle..." />
    <br />
    <label>Date</label>
    <input type="text" name="Date" class="form-control" placeholder="Write to here Date..."/>
    <br />
    <label>Description</label>
    <textarea name="Description" class="form-control" placeholder="Write to here Description..." rows="5"> </textarea>
    <br />
    <button class="btn-info">Save</button>
</form>