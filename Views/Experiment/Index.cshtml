@using DynamicCv.Models.Entity
@model List<TblExperience>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h2>Experiments List</h2>
<table class="table table-bordered"> 
    <tr>
        <th>ID</th>
        <th>Title</th>
        <th>SubTitle</th>
        <th>Date</th>
        <th>Delete</th>
        <th>Edit</th>
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.Id</td>
            <td>@x.Title</td>
            <td>@x.SubTitle</td>
            <td>@x.Date</td>
            <td><a href="/Experiment/DeleteExperiment/@x.Id/" class="btn btn-danger">Delete</a></td>
            <td><a href="/Experiment/EditExperiment/@x.Id" class="btn btn-warning">Edit</a></td>
        </tr>
    }
</table>
<br />
<a href="/Experiment/AddExperiment/" class="btn btn-primary">Add Experiment</a>